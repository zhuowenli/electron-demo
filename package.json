{
  "private": true,
  "name": "ddzs",
  "description": "多多助手",
  "version": "0.1.0",
  "scripts": {
    "postinstall": "cd app && npm install && rm -rf package-lock.json",
    "test": "echo lol",
    "build": "poi build",
    "renderer": "poi",
    "dev": "run-p renderer main -r",
    "main": "electron --inspect=5858 app/index.js",
    "dist": "build"
  },
  "build": {
    "appId": "com.lvjian.ddzs",
    "productName": "多多助手",
    "asar": true,
    "mac": {
      "category": "public.app-category.developer-tools"
    },
    "win": {
      "target": "nsis"
    },
    "nsis": {
      "oneClick": false
    },
    "linux": {
      "category": "public.app-category.developer-tools"
    }
  },
  "devDependencies": {
    "autoprefixer": "^8.2.0",
    "babel-cli": "^6.26.0",
    "babel-eslint": "^8.2.2",
    "babel-plugin-component": "^1.1.0",
    "babel-plugin-transform-runtime": "^6.23.0",
    "babel-preset-env": "^1.6.1",
    "electron": "^2.0.0-beta.7",
    "electron-builder": "^18.0.1",
    "electron-builder-squirrel-windows": "^18.0.0",
    "eslint": "^4.19.1",
    "eslint-config-airbnb-base": "^12.1.0",
    "eslint-config-vue": "^2.0.2",
    "eslint-plugin-import": "^2.10.0",
    "eslint-plugin-vue": "^4.4.0",
    "node-sass": "^4.8.3",
    "npm-run-all": "^4.1.2",
    "poi": "^9.2.0",
    "postcss-pxtorem": "^4.0.1",
    "pug": "^2.0.3",
    "sass-loader": "^6.0.7"
  },
  "dependencies": {
    "axios": "^0.18.0",
    "bluebird": "^3.5.1",
    "element-ui": "^2.3.3",
    "normalize.css": "^8.0.0",
    "superagent": "^3.8.2",
    "superagent-charset": "^1.2.0",
    "vue-router": "^3.0.1",
    "vuex": "^3.0.1",
    "vuex-router-sync": "^5.0.0"
  }
}
